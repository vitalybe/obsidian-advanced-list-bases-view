    "plugin",
    "list",
    "bases",
    "view"
  ],
  "author": "Obsidian",
  "authorUrl": "https://obsidian.md",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.6.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.10",
    "obsidian": "latest",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "maplibre-gl": "5.8.0"
  }
}
